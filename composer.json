{
    "name": "k2gl/entity-exist",
    "description": "Checks if there is at least one row with the required field in the database, for example, a row with the same identifier",
    "type": "composer-plugin",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "composer-plugin-api": "^2.0",
        "doctrine/orm": "^2.13",
        "php": "^8.1",
        "symfony/validator": "^6.1"
    },
    "require-dev": {
        "composer/composer": "^2.4.3",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/var-dumper": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "K2gl\\Component\\Validator\\Constraint\\EntityExist\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "K2gl\\Component\\Validator\\Constraint\\EntityExist\\Test\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Nickolay Harin",
            "email": "l0gic@ya.ru"
        }
    ]
}
